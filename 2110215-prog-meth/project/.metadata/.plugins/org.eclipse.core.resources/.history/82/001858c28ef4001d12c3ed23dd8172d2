package entity.unit;

import config.Config;

public class SwordMan extends BaseUnit implements Movable, Attackable{

	public SwordMan () {
		super(Config.MILITARY_SIZE, Config.SWORDMAN_ATTACK_RANGE, Config.SWORDMAN_MOVE_RANGE, Config.SWORDMAN_ATTACK_MULTIPLIER,  false);
	}
	
	// attack and move can be used by calling "super" for BaseUnit's method (The logics are the same.)
}
