package entity.unit;

import utils.GameConfig;

public class Archer extends BaseUnit implements Movable, Attackable{

	/**
	 * The constructor for this class.
	 * 
	 */
	public Archer() {
		super(GameConfig.MILITARY_SIZE, GameConfig.ARCHER_ATTACK_RANGE, GameConfig.ARCHER_MOVE_RANGE, GameConfig.ARCHER_ATTACK_MULTIPLIER);
	}
	
	/**
	 * The constructor for this class.
	 * 
	 * @param people 				Amount of people of this entity
	 * @param attackRange         	Attack Range of this entity
	 * @param moveRange				Move value of this entity
	 * @param attackMultiplier          Defense value of this entity
	 */
	public Archer(int people, int attackRange, int moveRange, float attackMultiplier) {
		super(people, attackRange, moveRange, attackMultiplier);
	}
}
